  <body>
   
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
          <a class="navbar-brand" href="/forum">iEducate</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="/home/<%= Suc.name %>">Home</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/aboutus">About</a>
              </li>
              <% if (Suc.login === 'true') { %>
                <li class="nav-item">
                    <a class="nav-link" href="/dashboard/<%= Suc.name %>">Dashboard</a>
                </li>
              <% } %>

              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                  Dropdown
                </a>
                <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                  <li><a class="dropdown-item" href="#">Action</a></li>
                  <li><a class="dropdown-item" href="#">Another action</a></li>
                  <li><hr class="dropdown-divider"></li>
                  <li><a class="dropdown-item" href="#">Something else here</a></li>
                </ul>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/contactus" tabindex="-1">Contact</a>
              </li>
            </ul>
            <div class="mx-2 d-flex">

                <form class="d-flex ">
                  <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                  <button class="btn btn-success " type="submit">Search</button>
                </form>
                    <% if(Suc.login != 'true' ) { %>
                    <button class="btn btn-outline-success mx-2" data-bs-toggle="modal" data-bs-target="#loginModal">Login</button>
                    <button class="btn btn-outline-success ml-2" data-bs-toggle="modal" data-bs-target="#signupModal">Signup</button>
                    <% } %>
                    <% if (Suc.login === 'true') { %>
                        <p class="text-light my-2 mx-2 text-center fs-6"> Welcome <%= Suc.name %></p>
                      <a href="/logout" class="btn btn-outline-success" role="button">Logout</a>
                    <% } %>
                </div>

            </div>
          </div>
          </nav>

          <%- include('partials/loginModal') %>
          <%- include('partials/signupModal') %>

 





        <% if(Suc.success === 'true'){ %>
         <div class="alert alert-success alert-dismissible fade show my-0" role="alert">
             <strong>Success!</strong> Account created successfully.
             <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
           </div>
        <% } %>

        <% if(Suc.failure === 'true'){ %>
            <div class="alert alert-danger alert-dismissible fade show my-0" role="alert">
                <strong>Failure!</strong> Account not created as password and confirm password do not match.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
              </div>
        <% } %>

        <% if(Suc.exist === 'true'){ %>
            <div class="alert alert-danger alert-dismissible fade show my-0" role="alert">
                <strong>Failure!</strong> Username already exits.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
              </div>
        <% } %>

        <% if(Suc.error1 === 'true'){ %>
            <div class="alert alert-danger alert-dismissible fade show my-0" role="alert">
                <strong>Failure!</strong> Username or Password incorrect.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
              </div>
        <% } %>

        <% if (Suc.login === 'true' && Suc.alert === 'true') { %>
            <div class="alert alert-success alert-dismissible fade show my-0" role="alert">
                <strong>Welcome!</strong> <%= Suc.name %> you can now access the dashboard to view more options.
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
              </div>
        <% } %>

